<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/dialog_bg">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/myacc"
        android:layout_centerHorizontal="true"/>
    <RelativeLayout
        android:id="@+id/first"
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_benefitpackage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Benefit Package:"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />
        <TextView
            android:id="@+id/benefitpackage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="\t\t\t\t\tClassic Program"
            android:textColor="#6d6767"
            android:layout_alignTop="@+id/tv_benefitpackage"
            android:layout_toRightOf="@+id/tv_roomlimit"
            android:layout_toEndOf="@+id/tv_roomlimit" />

        <TextView
            android:id="@+id/tv_plantype"
            android:layout_below="@+id/tv_benefitpackage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Plan Type:"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />
        <TextView
            android:id="@+id/plantype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="\t\t\t\t\tDiamond"
            android:textColor="#6d6767"
            android:layout_above="@+id/tv_roomtype"
            android:layout_alignLeft="@+id/benefitpackage"
            android:layout_alignStart="@+id/benefitpackage" />
        <TextView
            android:id="@+id/tv_roomtype"
            android:layout_below="@+id/tv_plantype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Room Type:"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />
        <TextView
            android:id="@+id/roomtype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="\t\t\t\t\tPrivate"
            android:textColor="#6d6767"
            android:layout_above="@+id/tv_roomlimit"
            android:layout_toRightOf="@+id/tv_roomlimit"
            android:layout_toEndOf="@+id/tv_roomlimit" />

        <TextView
            android:id="@+id/tv_roomlimit"
            android:layout_below="@+id/tv_roomtype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Room &amp; Board Limit :"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />

        <TextView
            android:id="@+id/roomlimit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/tv_roomlimit"
            android:layout_alignBottom="@+id/tv_roomlimit"
            android:layout_alignLeft="@+id/roomtype"
            android:text="\t\t\t\t\t2500"
            android:layout_marginTop="5dp"
            android:textColor="#6d6767"/>
        <TextView
            android:id="@+id/tv_hospital"
            android:layout_below="@+id/tv_roomlimit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hospital:"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />

        <TextView
            android:id="@+id/hospital"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/tv_hospital"
            android:layout_alignBottom="@+id/tv_hospital"
            android:layout_alignLeft="@+id/roomtype"
            android:text="\t\t\t\t\tNOAHMC"
            android:layout_marginTop="5dp"
            android:textColor="#6d6767"/>
        <TextView
            android:id="@+id/tv_benefitlimit"
            android:layout_below="@+id/tv_hospital"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Benefit Limit:"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="#008444"
            />

        <TextView
            android:id="@+id/benefitlimit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/tv_benefitlimit"
            android:layout_alignBottom="@+id/tv_benefitlimit"
            android:layout_alignLeft="@+id/roomtype"
            android:text="\t\t\t\t\t250000"
            android:layout_marginTop="5dp"
            android:textColor="#6d6767"/>
    </RelativeLayout>
</RelativeLayout>
